{
  "name": "@thecloudmethod/commander-print-proxy",
  "version": "0.1.0",
  "description": "A RESTful api proxy to generate print files to Bartenders Commander watch folder queue.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thecloudmethod/commander-print-proxy.git"
  },
  "contributors": [
    "Ben Morgan <ben@thecloudmethod.com>"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disableHostCheck true",
    "start:server": "nodemon",
    "serve": "node serve-script",
    "build": "ng build",
    "lint": "ng lint base-web",
    "build:client-and-server-bundles": "ng build --prod && ng run base-web:server:production",
    "build:prerender": "yarn build:client-and-server-bundles && yarn webpack:server && yarn generate:prerender",
    "build:ssr": "yarn build:client-and-server-bundles && yarn webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server -p 8080",
    "serve:ssr": "node dist/server"
  },
  "pre-commit": [],
  "private": true,
